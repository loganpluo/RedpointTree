<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RedPoint">
        <attr name="id" format="string|reference" />
        <attr name="needCache" format="boolean" />
    </declare-styleable>

    <declare-styleable name="RedPointTextView">
        <attr name="redPointTreeName" format="string|reference" />
        <attr name="redPointId" format="string|reference" />
        <attr name="redPointStyle" format="enum" >
            <enum name="unreadcount" value="0"/>
            <enum name="point" value="1"/>
        </attr>
    </declare-styleable>

</resources>